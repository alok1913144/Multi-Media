<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_margin="5dp"
        app:cardCornerRadius="4dp"
        android:id="@+id/cv_logout">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Related"
            android:id="@+id/related_f2"
            android:textColor="@color/red"
            android:textSize="25sp"
            android:drawableStart="@drawable/ic_baseline_bookmarks_24"
            android:paddingStart="10dp"
            android:drawablePadding="10dp"
            android:padding="5dp"/>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_margin="5dp"
        app:cardCornerRadius="4dp"
        android:id="@+id/cv_privacy">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Your Questions"
            android:textColor="@color/red"
            android:textSize="25sp"
            android:id="@+id/your_Question_f2"
            android:drawableStart="@drawable/ic_baseline_restore_24"
            android:paddingStart="10dp"
            android:drawablePadding="10dp"
            android:padding="5dp"/>
    </androidx.cardview.widget.CardView>






</LinearLayout>